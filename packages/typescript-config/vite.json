{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2022",
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "ES2022"],
    "noEmit": true,
    "resolveJsonModule": true,
    "types": ["vite/client"]
  }
}
